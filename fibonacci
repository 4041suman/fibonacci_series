import java.util.*;
class fibonacci{
    public static void main(String[] args){
       for(int i=0;i<=7;i++){
           System.out.print(sum(i)+" ");
       }
    }
    public static int sum(int i){
        if(i<=1){
            return i;
        }
        else{
           return sum(i-1)+sum(i-2);
    }
    }
}
